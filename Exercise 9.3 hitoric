library(rpart)
library(rpart.plot)
#Exercise 9.3
#a)
library(rpart.plot)
library(rpart)
car.df<-read.csv("ToyotaCorolla.csv")
selected.var<-c(3, 4, 7, 8, 9, 12, 14, 17, 19, 21, 25, 26, 28, 30, 34, 39)
set.seed(1)
train.rows<-sample(rownames(car.df), dim(car.df)[1]*0.6)
valid.rows<-setdiff(rownames(car.df), train.rows)
train.data<-car.df[train.rows, selected.var]
valid.data<-car.df[valid.rows, selected.var]
car.rt<- rpart (Price~., data=train.data, method ="annova", cp=0.001,minsplit=1,maxsplit=100
)
car.rt<- rpart (Price~., data=train.data, method ="annova", cp=0.001,minsplit=1,maxsplit=100)
car.rt<- rpart (Price~., data=train.data, method ="annova", cp=0.001,minsplit=1)
car.rt<- rpart (Price~., data=train.data, method ="anova", cp=0.001,minsplit=1,maxsplit=100)
car.rt<- rpart (Price~., data=train.data, method ="anova", cp=0.001,minsplit=1)
prp(car.rt, type = 1, extra = 1, under = TRUE, split.font = 1, varlen = -10)
car.rt<- rpart (Price~., data=train.data, method ="class", cp=0.001,minsplit=1)
prp(car.rt, type = 1, extra = 1, under = TRUE, split.font = 1, varlen = -10)
prp(car.rt, type = 1, extra = 1, under = TRUE, split.font = 1, varlen = -10)
car.rt<- rpart (Price~., data=train.data, method ="anova", cp=0.001,minsplit=1, maxdepth=100)
car.rt<- rpart (Price~., data=train.data, method ="anova", cp=0.001,minsplit=1, xval=5)
prp(car.rt, type = 1, extra = 1, under = TRUE, split.font = 1, varlen = -10)
car.rt<- rpart (Price~., data=train.data, method ="anova", cp=0.001,minsplit=1, xval=100)
prp(car.rt, type = 1, extra = 1, under = TRUE, split.font = 1, varlen = -10)
print(car.rt)
car.rt<- rpart (Price~., data=train.data, method ="class", cp=0.001,minsplit=1, xval=100)
print(car.rt)
library(rpart.plot)
library(rpart)
car.df<-read.csv("ToyotaCorolla.csv")
selected.var<-c(3, 4, 7, 8, 9, 12, 14, 17, 19, 21, 25, 26, 28, 30, 34, 39)
set.seed(1)
train.rows<-sample(rownames(car.df), dim(car.df)[1]*0.6)
valid.rows<-setdiff(rownames(car.df), train.rows)
train.data<-car.df[train.rows, selected.var]
valid.data<-car.df[valid.rows, selected.var]
car.rt<- rpart (Price~., data=train.data, method ="anova", cp=0.001,minbucket=1)
prp(car.rt, type = 1, extra = 1, under = TRUE, split.font = 1, varlen = -10)
print(car.rt)
#Exercise 9.3
#a)
library(rpart.plot)
library(rpart)
car.df<-read.csv("ToyotaCorolla.csv")
selected.var<-c(3, 4, 7, 8, 9, 12, 14, 17, 19, 21, 25, 26, 28, 30, 34, 39)
set.seed(1)
train.rows<-sample(rownames(car.df), dim(car.df)[1]*0.6)
valid.rows<-setdiff(rownames(car.df), train.rows)
train.data<-car.df[train.rows, selected.var]
valid.data<-car.df[valid.rows, selected.var]
car.rt<- rpart (Price~., data=train.data, method ="anova", cp=0.001,minbucket=1)
prp(car.rt, type = 1, extra = 1, under = TRUE, split.font = 1, varlen = -10)
summary(car.rt)
